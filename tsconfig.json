{
  "compilerOptions": {
    "outDir": "static/js",
    "rootDir": "static/ts",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true, 
    "strict": true,
    "skipLibCheck": true,
    "module": "es2020",
    "target": "es6",
    "moduleResolution": "node"
  },
  "include": ["static/ts/**/*"]
}

